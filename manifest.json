{
	"manifest_version" : 2,
	"name" : "To Do List",
	"description" : "To Do list in chrome",
	"version" :"1.0",
	"permissions" :[
					"tabs",
					"storage"
					],
	"background":{
		"scripts":["background.js"]
	},
	"browser_action":{
					"default_icon":"icon.png",
					"default_popup":"popup.html",
					"default_name":"To Do list"
	}
}
